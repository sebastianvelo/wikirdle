{
  "name": "wikirdle",
  "version": "1.0.0",
  "description": "Wikirdle",
  "keywords": [
    "typescript",
    "react",
    "starter"
  ],
  "main": "src/index.tsx",
  "dependencies": {
    "axios": "^1.9.0",
    "loader-utils": "^3.3.1",
    "lucide-react": "^0.511.0",
    "postcss-import": "^16.1.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@types/react": "19.1.6",
    "@types/react-dom": "^18.2.15",
    "@types/webpack-dev-server": "^4.7.2",
    "autoprefixer": "^10.4.21",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.8",
    "ts-loader": "^9.5.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.99.9",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "scripts": {
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && firebase deploy",
    "tw:build": "npx tailwindcss -i ./src/styles.css -o ./src/output.css",
    "tw:watch": "npx tailwindcss -i ./src/styles.css -o ./src/output.css --watch",
    "start": "webpack serve --mode development",
    "build": "npm run tw:build && webpack --mode production",
    "dev": "webpack --mode development"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
